{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["CI", "TW_SECRET_KEY"],
  "tasks": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "bench": {
      "cache": false,
      "dependsOn": []
    },
    "typedoc": {
      "outputs": ["typedoc/**"],
      "dependsOn": ["typedoc-gen#build", "^build"]
    },
    "thirdweb-dashboard#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "portal#build": {
      "outputs": [".next/**", "!.next/cache/**", "searchIndex.json"],
      "dependsOn": ["^build", "thirdweb#typedoc"]
    },
    "portal#dev": {
      "dependsOn": ["^build", "thirdweb#typedoc"]
    },
    "playground-web#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "wallet-ui#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "@thirdweb-dev/service-utils#build": {
      "outputs": ["dist/**", "cf-worker/**", "node/**"],
      "dependsOn": ["^build"]
    },
    "thirdweb#update-version": {
      "inputs": ["package.json"],
      "outputs": ["src/version.ts"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "inputs": ["src/**", "test/**"],
      "dependsOn": ["^build"]
    },
    "storybook": {
      "inputs": ["src/**"]
    },
    "test:legacy": {
      "outputs": ["coverage/**"],
      "inputs": ["src/**", "test/**"],
      "dependsOn": ["^build"]
    },
    "e2e": {
      "outputs": [],
      "inputs": ["src/**", "fixtures/**"],
      "dependsOn": ["^build"]
    },
    "e2e:legacy": {
      "outputs": [],
      "inputs": ["src/**", "fixtures/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:legacy": {
      "cache": false,
      "outputs": []
    },
    "fix": {
      "cache": false,
      "outputs": []
    },
    "format": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "game-expo#ios": {
      "cache": false,
      "persistent": true
    },
    "game-expo#android": {
      "cache": false,
      "persistent": true,
      "env": ["ANDROID_HOME"]
    },
    "game-expo#web": {
      "cache": false,
      "persistent": true
    },
    "thirdweb-dashboard#dev": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
